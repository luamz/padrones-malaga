<p id="notice"><%= notice %></p>

<h1>Residencias</h1>

<table>
  <thead>
    <tr>
      <th>Registro</th>
      <th>Numero</th>
      <th>Piso</th>
      <th>Dpto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @residencias.each do |residencia| %>
      <tr>
        <td><%= residencia.registro_id %></td>
        <td><%= residencia.numero %></td>
        <td><%= residencia.piso %></td>
        <td><%= residencia.dpto %></td>
        <td><%= link_to 'Show', residencia %></td>
        <td><%= link_to 'Edit', edit_residencia_path(residencia) %></td>
        <td><%= link_to 'Destroy', residencia, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Residencia', new_residencia_path %>
